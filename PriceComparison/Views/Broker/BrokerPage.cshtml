                    <!-- Form -->
                    <div class="form-container mb-5" id="form">
                        <form>
                            <div class="mb-3 col-md-3">
                                <label for="age" class="form-label">גיל</label>
                                <input type="number"
                                       class="form-control form-control-xl"
                                       id="age"
                                       value="30" min="0" max="95" />
                            </div>

                            <div class="mb-3">
                                <label class="form-label passenger-label">נוסע/ת 1</label>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-12">
                                    <div class="form-check form-check-reverse">
                                        <input class="form-check-input"
                                               type="checkbox"
                                               id="checkbox1"
                                               checked disabled />
                                        <label class="form-check-label" for="checkbox1">
                                            הוצאות רפואיות בחו"ל
                                        </label>
                                    </div>
                                    <div class="form-check form-check-reverse">
                                        <input class="form-check-input"
